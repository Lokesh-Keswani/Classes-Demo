{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/sta",
      "dest": "/index.html"
    },
    {
      "src": "/courses",
      "dest": "/index.html"
    },
    {
      "src": "/faculty",
      "dest": "/index.html"
    },
    {
      "src": "/achievements",
      "dest": "/index.html"
    },
    {
      "src": "/students",
      "dest": "/index.html"
    },
    {
      "src": "/contact",
      "dest": "/index.html"
    },
    {
      "src": "/about",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
